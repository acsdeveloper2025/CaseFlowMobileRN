# Environment Variables Template
# Copy this file to .env and fill in your actual values

# ===========================================
# API Configuration
# ===========================================

# Google Gemini AI API Key
# Get your API key from: https://makersuite.google.com/app/apikey
API_KEY=your_gemini_api_key_here

# ===========================================
# App Configuration
# ===========================================

# App Environment (development, staging, production)
NODE_ENV=development

# App Version
APP_VERSION=1.0.0

# App Name
APP_NAME=CaseFlow Mobile

# ===========================================
# Backend API Configuration
# ===========================================

# Base URL for your backend API
API_BASE_URL=https://api.example.com

# API Timeout (in milliseconds)
API_TIMEOUT=30000

# ===========================================
# Authentication Configuration
# ===========================================

# JWT Secret (for local development only)
JWT_SECRET=your_jwt_secret_here

# OAuth Configuration (if using OAuth)
OAUTH_CLIENT_ID=your_oauth_client_id
OAUTH_CLIENT_SECRET=your_oauth_client_secret

# ===========================================
# Database Configuration (if applicable)
# ===========================================

# Database URL (for local development)
DATABASE_URL=your_database_url_here

# ===========================================
# Third-party Services
# ===========================================

# Analytics
ANALYTICS_KEY=your_analytics_key_here

# Crash Reporting
CRASHLYTICS_KEY=your_crashlytics_key_here

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key_here

# ===========================================
# Feature Flags
# ===========================================

# Enable/disable features
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEBUG_MODE=false

# ===========================================
# Build Configuration
# ===========================================

# Code Push Deployment Keys (for OTA updates)
CODEPUSH_DEPLOYMENT_KEY_ANDROID=your_android_deployment_key
CODEPUSH_DEPLOYMENT_KEY_IOS=your_ios_deployment_key

# Bundle ID / Package Name
BUNDLE_ID=com.caseflowmobilern
PACKAGE_NAME=com.caseflowmobilern
